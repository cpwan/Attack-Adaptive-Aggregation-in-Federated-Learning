# The log root directory:
LOGROOT: logs

# Code to ignore for copy
CODE_IGNORE_PATTERNS: '*.pth,*.pyc,docs,test,.git,*__pycache__,logs,AggData,data,results,*.pdf,*.ipynb,*.sh,*.pt'

# Farm-specific items:
FARM: myfarm

myfarm:
    SUBMIT_CMD: 'submit_job'
    RESOURCES:
        image: some_docker_image
        gpu: 2
        mem: 100

